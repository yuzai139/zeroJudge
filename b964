#include<bits/stdc++.h>
using namespace std;
int main()
{
    int stunum=0;
    cin>>stunum;
    int stu[19];
    int maxfalse,minpass;  
    int a=0,b=0;   //判別及格
    
    for(int i=0;i<stunum;i++)      //輸入分數
    {
        cin>>stu[i];
    }
    
    sort(stu,stu+stunum);          //排序
    
    for(int i=0;i<stunum;i++)      //輸出排序後分數
    {
        cout<<stu[i]<<" ";
    }

    cout<<endl;
    
    for(int i=0;i<stunum;i++)      //判斷出最高分不及格數以及是否都及格
    {
        if(stu[i]<60)
        {
            maxfalse=stu[i];
        }
    }
    
    if(stu[0]>=60)
    cout<<"best case"<<endl;
    else
    cout<<maxfalse<<endl;
    
    for(int i=stunum-1;i>=0;i--)   //判斷最低分及格和是否都不及格
    {
        if(stu[i]>=60)
        {
            minpass=stu[i];
        }
    }
    
    if(stu[stunum-1]<60)
    cout<<"worst case"<<endl;
    else
    cout<<minpass<<endl;
    
	return 0;
}
